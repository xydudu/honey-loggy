{
  "name": "honey-loggly",
  "version": "1.0.0",
  "description": "honey log service",
  "bin": "./bin/honey-log-server",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "babel": "babel-node ./index.js",
    "build": "cp ./package.json ./bin/package.json && babel ./index.js --out-file ./bin/honey-log-server && babel ./src --out-dir ./bin/src",
    "server": "node ./bin/honey-log-server"
  },
  "keywords": [
    "log"
  ],
  "author": "Lian Hsueh <xuedudu@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-root-import": "^4.1.3",
    "mocha": "^3.1.2",
    "redis": "^2.6.3"
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "transform-async-to-generator",
      "syntax-async-functions",
      "babel-root-import"
    ]
  },
  "server": {
    "port": "8124",
    "host": "0.0.0.0"
  },
  "redis_conf": {
    "port": "6379",
    "host": "0.0.0.0"
  },
  "dependencies": {
    "bluebird": "^3.4.6"
  }
}
